{
  "all": true,
  "check-coverage": true,
  "include": [
    "src/**/*.js"
  ],
  "exclude": [
    "**/*.spec.js",
    "**/*.test.js",
    "src/console/tests/test-app-stub"
  ],
  "reporter": [
    "json",
    "lcov",
    "text-summary",
    "clover"
  ],
  "branches": 50,
  "lines": 70,
  "functions": 65,
  "statements": 70
}
